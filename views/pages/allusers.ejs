<% extend ('../layout')%>

<div id="page-wrapper">
    <div id="page-inner">
        <div class="row">
            <div class="col-md-12">
                <h1>All Users</h1>
                <table class="table table-striped table-bordered">
                    <thead>
                        <tr>
                            <td>Email</td>
                            <td>First Name</td>
                            <td>Last Name</td>
                            <td>Phone Number</td>
                        </tr>
                    </thead>
                    <tbody>
                        <%d.forEach(a => {%>
                            <tr>
                            <td><%=a.val().email%></td>
                            <td><%=a.val().firstName%></td>
                            <td><%=a.val().lastName%></td>
                            <td><%=a.val().phNo%></td>
                        </tr>
                        <%});%>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>